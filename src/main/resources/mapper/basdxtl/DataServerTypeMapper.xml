<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.crm.mapper.basdxtl.DataServerTypeMapper">

    <delete id="deleteDataServerTypeBatch" parameterType="int">
    delete from DataServerType where dstId in (
    <foreach collection="array" item="dstId" separator=",">
    	#{dstId}
    </foreach>
    )
    </delete>
  
</mapper>